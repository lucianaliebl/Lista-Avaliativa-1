#include <stdio.h>

int main() {
    int senhaCadastrada;
    int senha; 

    scanf("%d", &senhaCadastrada);
    printf("senha cadastrada: %d\n", senhaCadastrada);

    scanf("%d", &senha);

    do{
       if(senha == senhaCadastrada){
         printf("senha valida!\n");
         break;
       }
       else{
         printf("senha  invalida!\n");
         scanf("%d", &senha); 
       }       
    } while (senha != senhaCadastrada); 
    return 0;
}
